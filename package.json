  "devDependencies": {
    "@nightwatch/vue": "^3.1.2",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "eslint": "^8.0.0",
    "prettier": "^2.0.0",
    "tslib": "^2.0.0",
    "vite": "^5.0.0",
    "vitest": "^3.0.5",
    "vue-tsc": "^0.30.0"
  }
}
